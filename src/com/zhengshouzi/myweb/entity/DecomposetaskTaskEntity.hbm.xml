<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.DecomposetaskTaskEntity" table="decomposetask_task" schema=""
           catalog="crowdsourcing">
        <composite-id mapped="true" class="com.zhengshouzi.myweb.entity.DecomposetaskTaskEntityPK">
            <key-property name="decomposeTaskId">
                <column name="decomposeTaskId" sql-type="bigint" not-null="true"/>
            </key-property>
            <key-property name="taskId">
                <column name="taskId" sql-type="bigint" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="whichStep">
            <column name="whichStep" sql-type="bigint"/>
        </property>
        <many-to-one name="decomposetaskByDecomposeTaskId" class="com.zhengshouzi.myweb.entity.DecomposetaskEntity">
            <column name="decomposeTaskId" not-null="true"/>
        </many-to-one>
        <many-to-one name="taskByTaskId" class="com.zhengshouzi.myweb.entity.TaskEntity">
            <column name="taskId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>