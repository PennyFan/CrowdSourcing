<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.MergetaskEntity" table="mergetask" schema="" catalog="crowdsourcing">
        <id name="id" column="id"/>
        <property name="finalResult" column="finalResult"/>
        <property name="releaseTime" column="releaseTime"/>
        <property name="deadlineTime" column="deadlineTime"/>
        <property name="completeTime" column="completeTime"/>
        <many-to-one name="taskByTaskId" class="com.zhengshouzi.myweb.entity.TaskEntity">
            <column name="taskId"/>
        </many-to-one>
    </class>
</hibernate-mapping>