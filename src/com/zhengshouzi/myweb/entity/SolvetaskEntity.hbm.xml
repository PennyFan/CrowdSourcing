<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.SolvetaskEntity" table="solvetask" schema="" catalog="crowdsourcing">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="result">
            <column name="result" sql-type="longtext" length="2147483647"/>
        </property>
        <property name="best">
            <column name="best" sql-type="enum" length="4"/>
        </property>
        <property name="releaseTime">
            <column name="releaseTime" sql-type="datetime"/>
        </property>
        <property name="deadlineTime">
            <column name="deadlineTime" sql-type="datetime"/>
        </property>
        <property name="completeTime">
            <column name="completeTime" sql-type="datetime"/>
        </property>
        <property name="taskId">
            <column name="taskId" sql-type="bigint"/>
        </property>
        <property name="userId">
            <column name="userId" sql-type="bigint"/>
        </property>
        <many-to-one name="userByUserId" class="com.zhengshouzi.myweb.entity.UserEntity">
            <column name="userId"/>
        </many-to-one>
        <many-to-one name="taskByTaskId" class="com.zhengshouzi.myweb.entity.TaskEntity">
            <column name="taskId"/>
        </many-to-one>
        <set name="votetaskSolvetasksById" inverse="true">
            <key>
                <column name="solveTaskId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.VotetaskSolvetaskEntity"/>
        </set>
    </class>
</hibernate-mapping>