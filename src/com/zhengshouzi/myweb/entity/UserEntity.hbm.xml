<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.UserEntity" table="user" schema="" catalog="crowdsourcing">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="activateCode">
            <column name="activateCode" sql-type="varchar"/>
        </property>
        <property name="status">
            <column name="status" sql-type="enum" length="2"/>
        </property>
        <property name="registerDate">
            <column name="registerDate" sql-type="datetime" not-null="true"/>
        </property>
        <set name="decomposetasksById" inverse="true">
            <key>
                <column name="userId"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.DecomposetaskEntity"/>
        </set>
        <set name="judgetasksById" inverse="true">
            <key>
                <column name="userId"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.JudgetaskEntity"/>
        </set>
        <set name="solvetasksById" inverse="true">
            <key>
                <column name="userId"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.SolvetaskEntity"/>
        </set>
        <set name="tasksById" inverse="true">
            <key>
                <column name="userId"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.TaskEntity"/>
        </set>
        <set name="votetasksById" inverse="true">
            <key>
                <column name="userId"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.VotetaskEntity"/>
        </set>
    </class>
</hibernate-mapping>