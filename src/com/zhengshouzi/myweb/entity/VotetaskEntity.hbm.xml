<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.VotetaskEntity" table="votetask" schema="" catalog="crowdsourcing">
        <id name="id" column="id"/>
        <property name="releaseTime" column="releaseTime"/>
        <property name="deadlineTime" column="deadlineTime"/>
        <property name="completeTime" column="completeTime"/>
        <many-to-one name="taskByTaskId" class="com.zhengshouzi.myweb.entity.TaskEntity">
            <column name="taskId"/>
        </many-to-one>
        <many-to-one name="userByUserId" class="com.zhengshouzi.myweb.entity.UserEntity">
            <column name="userId"/>
        </many-to-one>
        <set name="votetaskDecomposetasksById" inverse="true">
            <key>
                <column name="voteTaskId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.VotetaskDecomposetaskEntity"/>
        </set>
        <set name="votetaskSolvetasksById" inverse="true">
            <key>
                <column name="voteTaskId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.zhengshouzi.myweb.entity.VotetaskSolvetaskEntity"/>
        </set>
    </class>
</hibernate-mapping>