<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhengshouzi.myweb.entity.VotetaskSolvetaskEntity" table="votetask_solvetask" schema=""
           catalog="crowdsourcing">
        <composite-id mapped="true" class="com.zhengshouzi.myweb.entity.VotetaskSolvetaskEntityPK">
            <key-property name="voteTaskId">
                <column name="voteTaskId" sql-type="bigint" not-null="true"/>
            </key-property>
            <key-property name="solveTaskId">
                <column name="solveTaskId" sql-type="bigint" not-null="true"/>
            </key-property>
        </composite-id>
        <many-to-one name="solvetaskBySolveTaskId" class="com.zhengshouzi.myweb.entity.SolvetaskEntity">
            <column name="solveTaskId" not-null="true"/>
        </many-to-one>
        <many-to-one name="votetaskByVoteTaskId" class="com.zhengshouzi.myweb.entity.VotetaskEntity">
            <column name="voteTaskId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>